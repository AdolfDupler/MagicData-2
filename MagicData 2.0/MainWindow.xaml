<Window x:Class="MagicData_2._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MagicData_2._0"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ProgressBar x:Name="loadingBar" HorizontalAlignment="Left" Height="40" Margin="10,270,0,0" VerticalAlignment="Top" Width="497" ValueChanged="loadingBar_ValueChanged"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,242,0,0" TextWrapping="Wrap" Text="Magic Data is loading..." VerticalAlignment="Top" Width="147"/>

    </Grid>
</Window>
